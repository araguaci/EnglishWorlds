{
    "name": "caddy-dz/english-worlds",
    "description": "Social network for non native English speakers",
    "keywords": ["social", "english"],
    "license": "GPL",
    "type": "project",
    "require": {
        "php": "^7.2",
        "cybercog/laravel-love": "^8.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "6.18.*",
        "laravel/tinker": "^2.0",
        "sentry/sentry-laravel": "^1.0.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^6.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "English\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files" : [
          "tests/helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('.env.dusk.local') || copy('.env.dusk.example', '.env.dusk.local');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
